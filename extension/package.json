{
  "name": "crosstab",
  "description": "Sync your tabs across devices and browsers",
  "author": "Lorenzo0111",
  "private": true,
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "build": "wxt build -b chrome && wxt build -b firefox && wxt build -b safari",
    "build:chrome": "wxt build -b chrome",
    "build:firefox": "wxt build -b firefox",
    "build:safari": "wxt build -b safari",
    "zip": "wxt zip -b chrome && wxt zip -b firefox && wxt zip -b safari",
    "zip:chrome": "wxt zip -b chrome",
    "zip:firefox": "wxt zip -b firefox",
    "zip:safari": "wxt zip -b safari",
    "compile": "tsc --noEmit",
    "postinstall": "wxt prepare"
  },
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.3",
    "@wxt-dev/auto-icons": "^1.0.2",
    "@wxt-dev/module-react": "^1.1.3",
    "typescript": "^5.8.3",
    "wxt": "^0.20.6"
  },
  "trustedDependencies": [
    "esbuild",
    "sharp",
    "spawn-sync"
  ]
}
